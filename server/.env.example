# The serial port where the Arduino is connected
SERIAL_PORT=COM3

# The baud rate for serial communication
# Should match the rate set in the Arduino code
BAUD_RATE=115200

# The current version of the application
# This is auto generated by the installer to assist version update notifications
VERSION=1.0.0

# Disable version checks
# Any assigned value will disable version checks
# Should only be used for development purposes
NO_VERSION_CHECK=true